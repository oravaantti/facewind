<h1>Listing articles</h1>
 
<table>
  <tr>
    <th>Username</th>
    <th>Password</th>
    <th>Salt</th>
    <th>Id</th>
  </tr>
 
  <% @users.each do |user| %>
    <tr>
      <td><%= user.username %></td>
      <td><%= user.password %></td>
      <td><%= user.salt %></td>
      <td><%= user._id %></td>
    </tr>
  <% end %>
</table>